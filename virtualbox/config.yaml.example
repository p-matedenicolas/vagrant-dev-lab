
user: "paulo"
sshKeyHostPath: "../ssh/id_rsa.pub"
sshKeyBoxPath: "/home/vagrant/.ssh/paulo.pub"

machines:
  bento-debian-server:
    number      : 1
    cpu         : 2
    mem         : 1024
    box         : bento/debian-12
